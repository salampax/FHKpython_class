    "uid": 5,
    "nid": 6
}

print(a["uid"])



diction = {"a": 1, "b": 2, "c": 3}
print(diction)


for i, j in diction.items():
    print(j)
    
    
    
b = diction["a"] 
c = b + 1



#字串反轉
a = "apple"

def reverse(string):
    result = string
    for c in string:
        result = c + result
    return result

k = reverse(a)
print(k)



